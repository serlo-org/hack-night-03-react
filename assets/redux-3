import { createStore, combineReducers } from 'redux'
import { Provider } from 'react-redux'

export default combineReducers({
  greeting: (state, action) => {
    switch (action.type) {
      case 'GREET':
        return action.text
    }
  },
  foo: (state = {
    bar = 'baz'
  }, action) => {
    return state
  }
})

const store = createStore(
  rootReducer,
  initialState,
)

ReactDOM.render((
  <Provider store={store}>
    <Greeting>
  </Provider >
), document.getElementById('app'))





















